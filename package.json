{
  "name": "swagger-extract-dto",
  "version": "1.0.2",
  "description": "extract DTO definitions from swagger json file",
  "main": "dist/swagger-extract-dto.js",
  "scripts": {
    "preinstall": "rollup -v",
    "xbuild:dev": "rollup -c",
    "xbuild:prod": "rollup -c --environment BUILD:production",
    "build:watch": "rollup -c --watch --no-watch.clearScreen",
    "xbuild": "npm run build:dev && npm run build:prod",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:petstore": "node ./dist/swagger-extract-dto.js -j -i -f ./src/sample/petstore.json -o ./tmp/petstore ",
    "test:github": "node ./dist/swagger-extract-dto.js -j -i -f ./src/sample/ghes-2.22.json -o ./tmp/github",
    "test:bitbucket": "node ./dist/swagger-extract-dto.js -j -i -f ./src/sample/bitbucket.json -o ./tmp/bitbucket",
    "test:gitlab": "node ./dist/swagger-extract-dto.js -j -i -f ./src/sample/gitlab.json -o ./tmp/gitlab",
    "test:heroku": "node ./dist/swagger-extract-dto.js -j -i -f ./src/sample/heroku.json -o ./tmp/heroku",
    "test:linkedin": "node ./dist/swagger-extract-dto.js -j -i -f ./src/sample/linkedin.json -o ./tmp/linkedin",
    "lint": "eslint ./src --ext .js,.jsx",
    "lint:fix": "eslint ./src --ext .js,.jsx --fix"
  },
  "bin": {
    "swagger-extract-dto": "dist/swagger-extract-dto.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paullasarev/swagger-extract-dto.git"
  },
  "keywords": [
    "swagger",
    "dto",
    "typescript",
    "definition"
  ],
  "author": "paullasarev@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paullasarev/swagger-extract-dto/issues"
  },
  "homepage": "https://github.com/paullasarev/swagger-extract-dto#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@types/deep-freeze": "^0.1.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "cross-env": "^7.0.3",
    "deep-freeze": "0.0.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.4",
    "json-schema-to-typescript": "^10.1.5",
    "json-schema-toolbox": "^1.0.1",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.58.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "eslint ./src --fix"
    ]
  },
  "dependencies": {
    "joi": "^17.4.2",
    "json-schema-ref-parser": "9.0.9",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "prettier": "2.4.1",
    "query-string": "7.0.1",
    "rimraf": "3.0.2",
    "typescript": "4.4.3",
    "yargs": "17.2.1"
  }
}
